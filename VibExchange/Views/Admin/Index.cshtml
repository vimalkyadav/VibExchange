@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_VibExchangeLayout.cshtml";
}
<div class="nav navbar-inner">
    <div class="container-fluid" style="overflow:auto;margin-top:20px;">
        <div class="col-md-10" id="divHome" style="border-left: solid 1px #4f9ae6; height:100vh; padding-top:15px;">           
            <div class="row" >
                <div class="col-md-10" id="divUsers" style="padding-left: 1px;">
                    <input type="button" value="Users Detail" id="btnAllUserDetail" class="btn-sm btn-info col-md-3" style="margin-left:2%" onclick="GetUserDetail()" />
                    <input type="button" value="Enquiry Detail" id="btnAllEnquiryDetail" class="btn-sm btn-info col-md-3" style="margin-left:2%" onclick="GetEnquiryDetail()"/>
                    <input type="button" value="Subscription Plans" id="btnAllUsersPlan" class="btn-sm btn-info col-md-3" style="margin-left:2%" />
                </div>
            </div>  
            <div class="row">
                <div style="padding-top:20px;padding-bottom:30px;" class="col-md-10">
                    <div id="detailDisplay" style="min-width:100%"> </div>
                </div>
            </div>         
        </div>
    </div>
</div>
<script type="text/javascript">
    function GetUserDetail() {
        $.ajax({
            url: "@Url.Action("GetUserDetail", "Admin")",
            type: 'GET',
            success: function (data) {
                $('#detailDisplay').html('');
                $('#detailDisplay').html(data);
            }
        });
    }
    function GetEnquiryDetail() {
        $.ajax({
            url: "@Url.Action("GetEnquiryDetail", "Admin")",
            type: 'GET',
        success: function (data) {
            $('#detailDisplay').html('');
            $('#detailDisplay').html(data);
        }
    });
    }
</script>

